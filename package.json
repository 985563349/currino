{
  "name": "currino",
  "version": "1.0.2",
  "description": "Functional style TypeScript utility library",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/currino.cjs",
  "unpkg": "./dist/currino.global.js",
  "jsdelivr": "./dist/currino.global.js",
  "module": "./dist/currino.js",
  "exports": {
    ".": {
      "import": "./dist/currino.js",
      "require": "./dist/currino.cjs"
    }
  },
  "types": "./dist/currino.d.cts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --sourcemap --dts",
    "test": "vitest",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/985563349/currino.git"
  },
  "keywords": [
    "currino"
  ],
  "author": "Jee",
  "license": "MIT",
  "homepage": "https://github.com/985563349/currino#readme",
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "release-it": "^15.10.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
